{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/home.html" },
    { "source": "/home", "destination": "/home.html" },
    { "source": "/about", "destination": "/about.html" },
    { "source": "/method", "destination": "/method_page.html" },
    { "source": "/urgent-kit", "destination": "/urgent_shout_kit.html" },
    { "source": "/masterclass", "destination": "/masterclass.html" },
    { "source": "/film", "destination": "/filmmoviepage.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/ebook", "destination": "/ebook.html" },
    { "source": "/getgift", "destination": "/getgift.html" },
    { "source": "/chp3", "destination": "/chp3.html" }
  ]
}
