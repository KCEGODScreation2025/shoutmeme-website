{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/home.html"
    },
    {
      "src": "/home",
      "dest": "/home.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/method",
      "dest": "/method_page.html"
    },
    {
      "src": "/urgent-kit",
      "dest": "/urgent_shout_kit.html"
    },
    {
      "src": "/masterclass",
      "dest": "/masterclass.html"
    },
    {
      "src": "/film",
      "dest": "/filmmoviepage.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/ebook",
      "dest": "/ebook.html"
    },
    {
      "src": "/getgift",
      "dest": "/getgift.html"
    },
    {
      "src": "/chp3",
      "dest": "/chp3.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
